{
  "name": "react-use-search-params-state",
  "version": "2.0.0",
  "description": "React hook to keep state using URL search params. Based on useSearchParams from react-router@7",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "test": "jest --config jestconfig.json",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run prettier && npm run lint"
  },
  "keywords": [
    "react",
    "react-router-v7",
    "typescript",
    "react-use-search-params-state"
  ],
  "author": "Juan Schwindt (jschwindt)",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.6",
    "@types/react": "^18.2.33",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jschwindt/react-use-search-params-state"
  },
  "bugs": {
    "url": "https://github.com/jschwindt/react-use-search-params-state/issues"
  },
  "homepage": "https://github.com/jschwindt/react-use-search-params-state#readme",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "react-router": "^7.0.2"
  }
}
